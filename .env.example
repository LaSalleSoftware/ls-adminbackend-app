APP_NAME=DummyAppName
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=DummyAppURL

LASALLE_APP_NAME=adminbackendapp
LASALLE_APP_DOMAIN_NAME=DummyLasalleAppDomainName
LASALLE_POPULATE_DATABASE_WITH_TEST_DATA=false
LASALLE_HOW_MANY_MINUTES_UNTIL_LOGINS_INACTIVITY=10
LASALLE_DUSK_TEST_BASE_PAUSE_IN_MILLISECONDS=500

//if you want the web middleware whitelist check to happen, then type in the full word "yes"
LASALLE_WEB_MIDDLEWARE_DO_WHITELIST_CHECK=no
