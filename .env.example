DB_DATABASE=DummyDbDatabase
DB_USERNAME=DummyDbUsername
DB_PASSWORD=DummyDbPassword

BROADCAST_DRIVER=log
CACHE_DRIVER=file
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
