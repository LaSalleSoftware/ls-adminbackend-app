// in the lasallesoftware-library config. List your whitelisted IP addresses as comma delimited without spaces!
LASALLE_WEB_MIDDLEWARE_WHITELIST_IP_ADDRESSES=151.101.193.153,151.101.193.154,151.101.193.154

NOVA_GUARD=web

LOG_CHANNEL=stack

DB_CONNECTION=mysql
