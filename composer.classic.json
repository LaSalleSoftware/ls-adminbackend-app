{
    "name": "lasallesoftware/ls-adminbackend-app",
    "description": "LaSalle Software Administrative Back-end Application.",
    "keywords": [
        "lasalle software",
        "south lasalle",
        "bob bloom",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Bob Bloom",
            "email": "bob.bloom@lasallesoftware.ca",
            "homepage": "https://LasalleSoftware.ca",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "fideloper/proxy": "^4.2",
        "laravel/framework": "^7.0",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "lcobucci/jwt": "^3.3",
        "league/flysystem-aws-s3-v3": "~1.0",
        "league/flysystem-cached-adapter": "~1.0",
        "lasallesoftware/ls-lasalleuibackend-pkg": "^2.0",
        "lasallesoftware/ls-librarybackend-pkg": "^2.0",
        "lasallesoftware/ls-novabackend-pkg": "^2.3",
        "lasallesoftware/ls-blogbackend-pkg": "^2.2",
        "lasallesoftware/ls-backup-pkg": "^2.2",
        "lasallesoftware/ls-contactformbackend-pkg": "^2.2",
        "laravel/nova": "~3.0",
        "laravel/ui": "^2.0"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5",
        "laravel/dusk": "^6.0",
        "laravel/tinker": "^2.0",
        "barryvdh/laravel-debugbar": "^3.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan nova:publish"
        ]
    }
}
